{
  "compilerOptions": {
    "target": "es2022",
    "useDefineForClassFields": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@rngds/header": ["apps/rngds//src/app/components/header/index.ts"],
      "@rngds/footer": ["apps/rngds//src/app/components/footer/index.ts"],
      "@rngds/home": ["apps/rngds//src/app/components/home/index.ts"],
      "@rngds/team": ["apps/rngds//src/app/components/team/index.ts"],
      "@rngds/training": ["apps/rngds//src/app/components/training/index.ts"],
      "@rngds/sponsoring": [
        "apps/rngds//src/app/components/sponsoring/index.ts"
      ],
      "@rngds/contact": ["apps/rngds//src/app/components/contact/index.ts"]
    }
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    },
    {
      "path": "./tsconfig.editor.json"
    }
  ],
  "extends": "../../tsconfig.base.json",
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
